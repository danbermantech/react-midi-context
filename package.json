    "@types/webmidi": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "2.1.1",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.3",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
