    "eslint-plugin-storybook": "0.6.13",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "json": "11.0.0",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "prop-types": "15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.9",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6",
    "vite": "5.4.6",
    "vite-plugin-dts": "3.5.1",
    "vitest": "2.1.1"
